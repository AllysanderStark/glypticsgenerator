<Window x:Class="GPG_Csharp._3DPreviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:h="clr-namespace:HelixToolkit.Wpf;assembly=HelixToolkit.Wpf"
        xmlns:local="clr-namespace:GPG_Csharp"
        mc:Ignorable="d"
        Title="_3DPreviewWindow" Height="450" Width="800">
    <Grid>
        <h:HelixViewport3D Name="viewport" Background="Black">
            <h:SunLight/>
            <h:FileModelVisual3D x:Name="fileModel"/>
        </h:HelixViewport3D>
    </Grid>
</Window>
